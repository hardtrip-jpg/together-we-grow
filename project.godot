"res://data/": "purple",
"res://documentation/": "gray",
"res://events/": "pink",
"res://globals/": "orange",
"res://scenes/": "pink",
"res://schedule/": "blue",
"res://scripts/": "green",
"res://ui/": "red"
}

[physics]

3d/physics_engine="Jolt Physics"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
